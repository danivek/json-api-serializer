{
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "no-param-reassign": 0,
    "max-len": [2, 200, 4, {
      "ignoreComments": true,
      "ignoreUrls": true
    }],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0
  }
}
